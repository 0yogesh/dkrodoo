<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="volunteer_wizard_view" model="ir.ui.view">
            <field name="name">volunteer.wizard.form.view</field>
            <field name="model">volunteer.wizard</field>
            <field name="arch" type="xml">
                <form string="Volunteer Wizard">
                    <sheet>
                        <group>
                            <field name="start_date"/>
                            <field name="end_date"/>
                        </group>
                    </sheet>

                    <footer>
                        <button type="object" name="action_create" string="Submit" class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>